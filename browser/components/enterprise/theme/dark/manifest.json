{
  "manifest_version": 2,
  "version": "1.0.0",
  "name": "Firefox Enterprise Dark",
  "theme": {
    "images": {
      "additional_backgrounds": ["background.png"]
    },
    "properties": {
      "additional_backgrounds_alignment": ["right top"],
      "additional_backgrounds_tiling": ["no-repeat"]
    },
    "colors": {
      "toolbar": "rgb(3, 51, 86)",
      "toolbar_text": "rgb(255, 255, 255)",
      "frame": "rgb(2, 34, 60)",
      "tab_background_text": "rgb(255, 255, 255)",
      "toolbar_field": "rgb(16, 26, 52)",
      "toolbar_field_text": "rgb(255, 255, 255)",
      "tab_line": "rgb(58, 126, 178)",
      "popup": "rgb(238, 245, 255)",
      "popup_text": "rgb(0, 0, 0)",
      "icons_attention": "rgb(36, 155, 228)",
      "ntp_background": "rgb(23, 32, 52)",
      "tab_loading": "rgb(58, 126, 178)"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "firefox-enterprise-dark@mozilla.org"
    }
  }
}
